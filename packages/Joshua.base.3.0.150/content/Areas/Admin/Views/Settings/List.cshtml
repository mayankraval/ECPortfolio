@{
  ViewBag.Title = "List";
  Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<div class="container">
  <div class="row">
    <h2>Setting List</h2>
  </div>
  <div class="row">
    <table class="table table-striped table-bordered table-hover">
      <thead>
        <tr>
          <th>Setting name</th>
          <th>Setting value</th>
        </tr>
      </thead>
      @foreach (var item in J.Settings.values.OrderBy(i => i.Key).ToList())
      {
        <tr>
          <td>@item.Key</td>
          <td>@item.Value</td>
        </tr>
      }
    </table>
  </div>
</div>

