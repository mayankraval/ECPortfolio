<h3>Google Settings</h3>

<div class="control-group">
  <label class="control-label" >Analytics Key</label>
  <div class="controls">
    <input type="hidden" name="setting-key" value="google:analyticskey" />
    <input type="text" name="setting-value" placeholder="Google analytics key" style="width: 80%;" value="@J.Settings["google:analyticskey"]"/>
  </div>
</div>

<div class="control-group">
  <label class="control-label" >Domain name without services (es: mysite.com)</label>
  <div class="controls">
    <input type="hidden" name="setting-key" value="google:analyticsdomain" />
    <input type="text" name="setting-value" placeholder="Google analytics domain name" style="width: 80%;" value="@J.Settings["google:analyticsdomain"]"/>
  </div>
</div>

<div class="control-group">
  <label class="control-label" >Google Map Key</label>
  <div class="controls">
    <input type="hidden" name="setting-key" value="google:mapkey" />
    <input type="text" name="setting-value" placeholder="" style="width: 80%;" value="@J.Settings["google:mapkey"]" />
  </div>
</div>

